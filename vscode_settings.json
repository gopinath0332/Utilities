{
    "workbench.iconTheme": "vscode-great-icons",
    "window.zoomLevel": -1,
    "files.hotExit": "onExit",
    "editor.fontSize": 18,
    "editor.tabSize": 3,
    "editor.lineHeight": 25,
    "editor.minimap.enabled": true,
    "editor.formatOnPaste": false,
    "editor.wordWrap": "on",
    "explorer.openEditors.visible": 20,
    "editor.suggestSelection": "first",
